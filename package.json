{
  "name": "nelsonbury",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node build/server/index",
    "dev": "npm-run-all --parallel copy-assets-and-watch-client watch:server watch",
    "build": "rimraf ./build && node ./scripts/copy-assets && npm run build:client && node ./scripts/hash-files && npm run build:server",
    "build:server": "BABEL_ENV=server webpack --config webpack.server.js --env.production",
    "build:client": "BABEL_ENV=client webpack --config webpack.client.js --env.production",
    "watch": "nodemon build/server/index.js --watch build",
    "watch:server": "webpack --config webpack.server.js --env.development --watch --progress --colors",
    "watch:client": "webpack --config webpack.client.js --env.development --watch --progress --colors",
    "eslint": "eslint app",
    "stylelint": "stylelint 'app/public/styles/**/*.scss'",
    "lint": "npm-run-all eslint stylelint --parallel",
    "test": "BABEL_ENV=test tape 'app/**/**/*.spec.js' -r ./tests/setup | tap-difflet -p",
    "test-and-lint": "npm run lint && npm test",
    "copy-assets-and-watch-client": "node ./scripts/copy-assets && npm run watch:client"
  },
  "dependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/register": "7.7.4",
    "@sendgrid/mail": "^7.4.5",
    "body-parser": "1.19.0",
    "classnames": "2.2.6",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-async-errors": "3.1.1",
    "express-session": "1.16.2",
    "express-sslify": "1.2.0",
    "form-data": "2.5.0",
    "foundation-sites": "6.5.3",
    "helmet": "3.15.0",
    "js-cookie": "2.2.0",
    "markdown-to-jsx": "^6.11.4",
    "node-fetch": "^2.6.1",
    "prop-types": "15.6.2",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.0.3",
    "react-router": "4.3.1",
    "react-router-config": "5.0.1",
    "react-router-dom": "4.3.1",
    "recaptcha-verify": "0.1.1",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "serve-static": "^1.14.1",
    "whatwg-fetch": "3.0.0",
    "yargs": "13.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.7.4",
    "autoprefixer": "9.4.7",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-rewire": "1.2.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-declaration-sorter": "4.0.1",
    "css-loader": "2.1.0",
    "csso-webpack-plugin": "1.0.0-beta.12",
    "eslint": "5.12.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "extract-css-chunks-webpack-plugin": "3.3.2",
    "html-minifier": "4.0.0",
    "husky": "1.3.1",
    "jsdom": "13.2.0",
    "jsdom-global": "3.0.2",
    "node-sass": "4.12.0",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "nyc": "14.0.0",
    "postcss-loader": "3.0.0",
    "react-testing-library": "5.4.4",
    "redux-mock-store": "1.5.3",
    "rimraf": "^2.6.3",
    "sass-loader": "7.1.0",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "tape": "4.9.2",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "tap-difflet": "^0.7.2"
  }
}
